{
  "manifest_version" : 2,
  "name" : "social-media-launcher",
  "description": "a quick launch to required social accounts" ,
  "version" : "1.0.0",
  "icons" : {
    "128" : "128.png"
  },
  "browser_action" : {
    "default_icon" : "icon.png",
    "default_popup" : "popup.html"
  },
  "permissions" : ["activeTab"]
}